{

  "manifest_version": 2,
  "name": "MoodleMassDownloader",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.ac.il/*"],
      "js": ["FileSaver.js", "./Stuk-jszip-3109282/dist/jszip.js", "jszip-utils.js" , "mrf.js"],
      "run_at": "document_end"
    }
  ],

    "permissions": [
        "*://*.ac.il/*cs/*",
        "downloads"
    ]

}
